{
  "name": "pay-frontend",
  "description": "Payments Frontend application",
  "version": "0.0.1-SNAPSHOT",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">5.6.0"
  },
  "standard": {
    "globals": [
      "describe",
      "context",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "it",
      "expect"
    ],
    "ignore": [
      "app/assets/**/*.js"
    ]
  },
  "scripts": {
    "compile": "grunt generate-assets",
    "clean": "grunt clean",
    "start": "node_modules/forever/bin/forever --minUptime 1000 --spinSleepTime 500 start.js",
    "stop": "node_modules/forever/bin/forever stop start.js",
    "watch": "chokidar app test *.js --initial -c 'npm run test'",
    "test": "npm run lint && npm run grunt-test",
    "lint": "standard",
    "watch-live-reload": "./node_modules/.bin/grunt watch",
    "grunt-test": "nyc node ./node_modules/.bin/grunt test",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "dependencies": {
    "accessible-autocomplete": "^1.6.0",
    "appmetrics": "1.1.2",
    "appmetrics-statsd": "1.0.1",
    "array.prototype.find": "2.0.x",
    "body-parser": "1.17.x",
    "change-case": "2.3.x",
    "client-sessions": "0.8.x",
    "compression": "1.6.x",
    "credit-card-type": "5.0.x",
    "csrf": "3.0.x",
    "express": "4.15.x",
    "forever": "0.15.x",
    "govuk_frontend_toolkit": "6.0.x",
    "govuk_template_mustache_inheritance": "https://github.com/jonheslop/govuk_template/releases/download/v0.22.4/mustache_inheritance_govuk_template-0.22.3.tgz",
    "hogan.js": "3.0.x",
    "humps": "^2.0.1",
    "i18n": "0.8.x",
    "latest-version": "3.1.x",
    "lodash": "4.17.x",
    "luhn": "2.1.x",
    "minimist": "1.2.x",
    "moment": "2.18.x",
    "morgan": "1.8.x",
    "pay-product-page": "https://github.com/alphagov/pay-product-page/releases/download/2.0.9/pay-product-page-2.0.9.tgz",
    "q": "1.5.x",
    "randomstring": "^1.1.5",
    "readdir": "0.0.x",
    "requestretry": "^1.12.0",
    "rfc822-validate": "1.0.x",
    "serve-favicon": "2.4.3",
    "staticify": "0.0.8",
    "string": "3.3.x",
    "throng": "4.0.x",
    "uk-postcode": "0.1.x",
    "winston": "2.3.x"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "snyk": "^1.36.0",
    "browserify": "14.4.x",
    "node-sass": "4.5.x",
    "chai": "^4.0.2",
    "jquery": "^3.2.1",
    "chai-as-promised": "^7.0.0",
    "chai-string": "^1.4.0",
    "cheerio": "^0.22.0",
    "chokidar-cli": "latest",
    "errorhandler": "^1.5.0",
    "express-writer": "0.0.4",
    "grunt": "1.0.x",
    "grunt-browserify": "5.0.x",
    "grunt-cli": "1.2.x",
    "grunt-concurrent": "2.3.x",
    "grunt-contrib-clean": "1.1.x",
    "grunt-contrib-compress": "1.4.x",
    "grunt-contrib-concat": "1.0.x",
    "grunt-contrib-copy": "1.0.x",
    "grunt-contrib-cssmin": "^2.2.1",
    "grunt-contrib-watch": "1.0.x",
    "grunt-env": "0.4.x",
    "grunt-mocha-test": "^0.13.2",
    "grunt-nightwatch": "^0.5.4",
    "grunt-nightwatch-report": "^0.1.6",
    "grunt-nodemon": "0.4.x",
    "grunt-rewrite": "1.0.x",
    "grunt-sass": "2.0.x",
    "grunt-text-replace": "0.4.x",
    "mocha": "^3.4.2",
    "mockserver-client": "^1.0.16",
    "mockserver-grunt": "^1.0.41",
    "nock": "^9.0.13",
    "nyc": "^11.0.3",
    "@pact-foundation/pact-node": "4.6.0",
    "portfinder": "^1.0.13",
    "proxyquire": "~1.8.0",
    "sass-lint": "^1.10.2",
    "should": "^11.2.1",
    "sinon": "^2.3.5",
    "standard": "^10.0.2",
    "superagent": "^3.5.2",
    "supertest": "^3.0.0",
    "pact": "1.0.0"
  },
  "nyc": {
    "reporter": "text",
    "check-coverage": false,
    "per-file": true,
    "lines": 85,
    "statements": 85,
    "functions": 85,
    "branches": 85
  },
  "snyk": true
}
