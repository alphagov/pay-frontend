    "proxyquire": "~2.0.1",
    "sass-lint": "^1.12.1",
    "sinon": "5.0.x",
    "snyk": "^1.78.x",
    "standard": "^11.0.x",
    "superagent": "^3.8.3",
    "supertest": "^3.0.0"
  },
  "nyc": {
    "reporter": "text",
    "check-coverage": false,
    "per-file": true,
    "lines": 85,
    "statements": 85,
    "functions": 85,
    "branches": 85
  },
  "snyk": true
}
