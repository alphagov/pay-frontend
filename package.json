{
  "name": "pay-frontend",
  "description": "Payments Frontend application",
  "version": "0.0.1-SNAPSHOT",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">8.11.1"
  },
  "standard": {
    "globals": [
      "describe",
      "context",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "it",
      "expect"
    ],
    "ignore": [
      "app/assets/**/*.js"
    ]
  },
  "scripts": {
    "compile": "grunt generate-assets",
    "clean": "grunt clean",
    "start": "node_modules/forever/bin/forever --minUptime 1000 --spinSleepTime 500 start.js",
    "stop": "node_modules/forever/bin/forever stop start.js",
    "watch": "chokidar app test *.js --initial -c 'npm run test'",
    "lint": "standard",
    "watch-live-reload": "./node_modules/.bin/grunt watch",
    "test": "nyc node ./node_modules/mocha/bin/mocha '!(node_modules)/**/*+(_test|_tests)'.js",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "dependencies": {
    "accessible-autocomplete": "^1.6.1",
    "appmetrics": "3.1.x",
    "appmetrics-statsd": "2.0.x",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "body-parser": "1.18.x",
    "change-case": "2.3.x",
    "client-sessions": "0.8.x",
    "compression": "1.7.x",
    "credit-card-type": "6.3.x",
    "csrf": "3.0.x",
    "express": "4.16.x",
    "forever": "0.15.x",
    "gaap-analytics": "^3.0.0",
    "humps": "^2.0.1",
    "i18n": "0.8.x",
    "lodash": "4.17.x",
    "mailcheck": "^1.1.1",
    "memory-cache": "^0.2.0",
    "minimist": "1.2.x",
    "morgan": "1.9.x",
    "nunjucks": "^3.1.2",
    "q": "1.5.x",
    "randomstring": "^1.1.5",
    "readdir": "0.1.x",
    "requestretry": "^1.13.0",
    "rfc822-validate": "1.0.x",
    "serve-favicon": "2.5.0",
    "staticify": "2.0.x",
    "throng": "4.0.x",
    "uk-postcode": "0.1.x",
    "winston": "2.4.x"
  },
  "devDependencies": {
    "@pact-foundation/pact-node": "6.16.0",
