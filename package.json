    "govuk_frontend_toolkit": "7.5.0",
    "govuk_template_jinja": "https://github.com/jonheslop/govuk_template/releases/download/v0.22.5/jinja_govuk_template-0.22.5.tgz",
    "grunt": "1.0.x",
    "grunt-browserify": "5.3.x",
    "grunt-cli": "1.2.x",
    "grunt-concurrent": "2.3.x",
    "grunt-contrib-clean": "1.1.x",
    "grunt-contrib-compress": "1.4.x",
    "grunt-contrib-concat": "1.0.x",
    "grunt-contrib-copy": "1.0.x",
    "grunt-contrib-cssmin": "^2.2.1",
