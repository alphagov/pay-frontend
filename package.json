    "babel-preset-es2015": "^6.24.1",
    "babelify": "8.0.x",
    "browserify": "16.2.x",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-string": "^1.4.0",
    "cheerio": "^1.0.0-rc.2",
    "chokidar-cli": "latest",
    "envfile": "^2.3.0",
    "eslint": "^4.19.1",
