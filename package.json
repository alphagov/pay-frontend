{
  "name": "pay-frontend",
  "description": "Payments Frontend application",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">8.11.3"
  },
  "standard": {
    "globals": [
      "describe",
      "context",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "it",
      "expect",
      "cy",
      "Cypress"
    ],
    "ignore": [
      "app/assets/**/*.js"
    ]
  },
  "scripts": {
    "compile": "grunt generate-assets",
    "clean": "grunt clean",
    "start": "node start.js",
    "watch": "chokidar app test *.js --initial -c 'npm run test'",
    "lint": "standard",
    "watch-live-reload": "./node_modules/.bin/grunt watch",
    "test": "DISABLE_APPMETRICS=true nyc node ./node_modules/mocha/bin/mocha '!(node_modules)/**/*+(_test|_tests|.test)'.js",
    "pact-stub": "docker run -t -p 8000:8000 -v \"$(pwd)/pacts/:/app/pacts\" pactfoundation/pact-stub-server:v0.0.10 -p 8000 -d pacts",
    "cypress:server": "DISABLE_APPMETRICS=true node --inspect -r dotenv/config start.js dotenv_config_path=test/cypress/test.env",
    "cypress:test": "cypress run",
    "cypress:test-headed": "cypress open",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect",
    "publish-pacts": "./bin/publish-pacts.js"
  },
  "dependencies": {
    "accessible-autocomplete": "^1.6.1",
    "appmetrics": "3.1.x",
    "appmetrics-statsd": "2.0.x",
    "aws-xray-sdk": "^1.2.0",
    "body-parser": "1.18.x",
    "change-case": "2.3.x",
    "client-sessions": "0.8.x",
    "compression": "1.7.x",
    "continuation-local-storage": "^3.2.1",
    "credit-card-type": "6.3.x",
    "csrf": "3.0.x",
    "express": "4.16.x",
    "gaap-analytics": "^3.0.0",
    "govuk-frontend": "^1.1.1",
    "humps": "^2.0.1",
    "i18n": "0.8.x",
    "lodash": "4.17.x",
    "mailcheck": "^1.1.1",
    "memory-cache": "^0.2.0",
    "minimist": "1.2.x",
    "morgan": "1.9.x",
    "nunjucks": "^3.1.2",
    "polyfill-array-includes": "^1.0.0",
    "promise-polyfill": "8.1.0",
    "randomstring": "^1.1.5",
    "readdir": "0.1.x",
    "requestretry": "^1.13.0",
    "rfc822-validate": "1.0.x",
    "serve-favicon": "2.5.0",
    "staticify": "2.0.x",
    "throng": "4.0.x",
    "uk-postcode": "0.1.x",
    "winston": "2.4.x"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.56",
    "@babel/preset-env": "^7.0.0-beta.56",
    "@pact-foundation/pact-node": "^6.20.0",
    "browserify": "16.2.x",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-string": "^1.4.0",
    "cheerio": "^1.0.0-rc.2",
    "chokidar-cli": "latest",
    "cypress": "^3.1.0",
    "dotenv": "^5.0.1",
    "envfile": "^2.3.0",
    "eslint": "^4.19.1",
    "govuk-country-and-territory-autocomplete": "^0.4.0",
    "grunt": "1.0.x",
    "grunt-babel": "^8.0.0-beta.0",
    "grunt-browserify": "5.3.x",
    "grunt-cli": "1.2.x",
    "grunt-concurrent": "2.3.x",
    "grunt-contrib-clean": "1.1.x",
    "grunt-contrib-compress": "1.4.x",
    "grunt-contrib-concat": "1.0.x",
    "grunt-contrib-copy": "1.0.x",
    "grunt-contrib-uglify": "^3.4.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-nodemon": "0.4.x",
    "grunt-rewrite": "1.0.x",
    "grunt-sass": "2.1.x",
    "mocha": "5.1.x",
    "nock": "9.2.5",
    "node-sass": "4.9.x",
    "nyc": "11.7.x",
    "pact": "4.3.2",
    "pay-product-page": "https://github.com/alphagov/pay-product-page/releases/download/2.1.54/pay-product-page-2.1.54.tgz",
    "proxyquire": "~2.0.1",
    "sass-lint": "^1.12.1",
    "sinon": "5.0.x",
    "snyk": "^1.78.x",
    "standard": "^11.0.x",
    "superagent": "^3.8.3",
    "supertest": "^3.0.0"
  },
  "nyc": {
    "reporter": "text",
    "check-coverage": false,
    "per-file": true,
    "lines": 85,
    "statements": 85,
    "functions": 85,
    "branches": 85
  },
  "snyk": true
}
